                        </div>
                        <div>
                            <a href='<%=ResolveClientUrl("Purchases.aspx")%>'>Purchases</a>
                        </div>
                        <div>
                            <a href='<%=ResolveClientUrl("Sales.aspx")%>'>Sales</a>
                        </div>
                        <div>
                            <a href='<%=ResolveClientUrl("UserManagement.aspx")%>'>User Management</a>
                        </div>
                        <div>
                            <a href='<%=ResolveClientUrl("Report")%>'>Report Portal</a>
                        </div>
                        
                        <!-- End Sidebar -->
                        <!--End nav content -->
                    </div>
                </div>
                <div class="col-md-10 mainCell no-sidebar">
                    <div style="float: left">
                        <asp:Label ID="lblMsg" runat="server"></asp:Label>
                    </div>
                    <div id="mainContent">
                        <!--Start main content -->
                        <div class="linktrail" style="padding: 0px 5px;height: 20px;">
                            <asp:SiteMapPath ID="SiteMapPath1" runat="server">
                            </asp:SiteMapPath>
                            <div style="float: right">
                                [<a href='<%=ResolveClientUrl("Account")%>'>
                                    <asp:LoginName ID="LoginName2" runat="server" />
                                </a>
                                ]&nbsp;&nbsp;&nbsp;<asp:LoginStatus ID="LoginStatus2" runat="server"  CssClass="glyphicon glyphicon-log-out" OnLoggedOut="LoginStatus2_LoggedOut" />
                            </div>
                        </div>
                        <asp:ContentPlaceHolder ID="MainContent" runat="server" />
                    </div>

                </div>
                <div id="footer" class="col-md-12">
                    <table summary="" border="0" cellpadding="0" cellspacing="0" width="100%">
                        <tbody>
                            <tr>
                                <td>
                                    <a href="#">
                                        <img src="Images/contact-icon.gif" alt="" title="" border="0">
                                        Contact the <em>Developer</em> Team</a>
