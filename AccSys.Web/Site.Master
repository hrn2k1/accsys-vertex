<head id="Head1" runat="server">
    <title>AccSys</title>
    <meta http-equiv="x-ua-compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0" />

    <link href="themes/redmond/jquery-ui-1.8.1.custom.css" rel="stylesheet" type="text/css" />
   <%--  <link href="Styles/jquery-ui-1.8.1.custom.css" rel="stylesheet" type="text/css" />--%>
    <link href="Styles/Grid.css" rel="stylesheet" type="text/css" />
    <script src="Scripts/jquery-1.6.2.js" type="text/javascript"></script>
    <script src="Scripts/jquery-ui-1.8.1.custom.min.js" type="text/javascript"></script>
      <%--<script src="//code.jquery.com/jquery-1.10.2.js"></script>
     <script src="Scripts/jquery-ui-1.8.1.custom.min.js" type="text/javascript"></script>--%>
    <link rel="stylesheet" type="text/css" href="Styles/TTL.css" />
    <script src="Scripts/TTL.js" type="text/javascript"></script>
    <link href="Content/ems.css" rel="stylesheet" />

    <link href="Content/bootstrap.min.css" rel="stylesheet" />
    <link href="Content/bootstrap-theme.min.css" rel="stylesheet" />
    <script type="text/javascript" src="Scripts/bootstrap.min.js"></script>
   
    <link href="css/chosen.css" rel="stylesheet" />
    <%--<link href="css/chosen.min.css" rel="stylesheet" />--%>
    <script type="text/javascript">
        $(document).ready(function () {

            $("input[type=text]").focus(function () { $(this).css({ "background-color": "Black", "color": "White" }); $(this).select(); });
            $("input[type=text]").blur(function () { $(this).css({ "background-color": "White", "color": "Black" }); });
        });
    </script>
    <style type="text/css">
        input[type="text"], input[type="password"], textarea {
            background-color: White;
            color: Black;
            padding-left: 5px;
        }

        input[disabled="disabled"] {
            background-color: InactiveBorder;
            border: none;
            color: Black;
        }

            input[disabled="disabled"]:hover {
                background-color: InactiveBorder;
                border: none;
                color: Black;
            }

        input[type="text"]:focus, input[type="password"]:focus, textarea:focus {
            background-color: Black;
            color: White;
        }

        input[type="text"]:blur, input[type="password"]:blur, textarea:blur {
            background-color: White;
            color: Black;
        }

        .logo {
            width: 100%;
            height: 65px;
            border-radius: 50%;
            border: 1px solid black !important;
        }

        .companyName {
            float: right;
            font-weight: bold;
            font-style: oblique;
            font-family: Georgia;
            font-size: 20pt;
            color: rgb(51, 102, 204);
            padding: 0px;
        }
        .chzn-container .chzn-drop {
            min-width: 500px;
        }
        body > div.container ~ * {
                display: none !important;
            }
        blink {
            -webkit-animation-name: blink; 
            -webkit-animation-iteration-count: infinite; 
            -webkit-animation-timing-function: cubic-bezier(1.0,0,0,1.0);
            -webkit-animation-duration: 1s;
            }
         [data-val-errormessage="*"] {
            color: red !important;
            font-weight: bolder !important;
            text-decoration: blink;
        }
    </style>
    <asp:ContentPlaceHolder ID="HeadContent" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <div class="container" id="pageTable" style="width: 95%;">
        <div class="row" style="margin-top: 5px; margin-right: 5px; margin-left: 0;">
            <div class="col-md-1" style="padding: 0">
                <asp:Image ID="ImgLogo" CssClass="logo" ImageUrl='~/Logo/logo.jpg' runat="server" />
            </div>
            <div class="col-md-6" style="padding: 0">
                <img src='<%=ResolveClientUrl("Images/logo.png") %>' alt="" usemap="#mwlogomap" />
                <map id="mwlogomap">
                    <area shape="rect" coords="0,30,200,47" href="#" alt="Built with Latest Technology"
                        title="Built with Leatest Experiences" />
                    <area shape="rect" coords="0,0,617,30" href="#" alt="" />
                </map>

            </div>
            <div class="col-md-5" style="padding: 0">
                <div id="searchWrapper" class="col-md-12">
                    <form name="NavBarSearch" action='<%=ResolveClientUrl("VoucherRegister.aspx") %>' method="get" accept-charset="UTF-8"
                        onsubmit=" if (checkForDefaultInput('query_1', 'Search vouchers')) { return false; }">
                        <input id="search" class="searchboxsub" name="query" value="Search vouchers" onfocus="initTextInput(this,'clear','Search vouchers')"
                            onblur="initTextInput(this,'setDefault','Search vouchers')" type="text" />
                        <input src='<%=ResolveClientUrl("Images/search-icon.png") %>' id="search-icon" alt="Search" onclick="if (checkForDefaultInput('query_1', 'Search Student')) { return false; } else { document.NavBarSearch.submit(); }"
                            type="image" />
                    </form>
                </div>
                <div class="col-md-12" style="padding: 0;white-space: nowrap;">
                    <asp:Label ID="lblCompanyName" runat="server" CssClass="companyName" Text="Vertex Group of Industries" ToolTip="Vertex Group of Industries"></asp:Label>
                </div>
            </div>
        </div>

        <form id="Form1" runat="server">
            <ajax:ToolkitScriptManager ID="ToolkitScriptManager1" runat="server" AsyncPostBackTimeout="0" CombineScripts="True">
            </ajax:ToolkitScriptManager>

            <div class="row">
                <div class="leftcolumn col-md-2">
                    <!--Start nav content -->
                    <!-- Sidebar -->
                    <div id="navigation">
                        <div>
                            <a href='<%=ResolveClientUrl("Default.aspx") %>'>Home</a>
                        </div>
                        <div>
                            <a href='<%=ResolveClientUrl("CompanyInfo.aspx")%>'>Company Information</a>
                        </div>
                        <div>
